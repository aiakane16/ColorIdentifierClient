{
  "expo": {
    "name": "Color Identifier",
    "slug": "ColorIdentifier",
    "privacy": "public",
    "entryPoint": "./index.js",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "",
    "android": {
      "package": "com.coloridentity.identifier",
      "versionCode": 1,
      "splash": {
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png",
        "resizeMode": "cover"
      }
    },
    "ios": {
      "bundleIdentifier": "sample-color-identifier"
    }
  }
}